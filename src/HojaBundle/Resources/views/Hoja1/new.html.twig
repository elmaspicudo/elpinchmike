{% extends '::base.html.twig' %}
{% block placeholder -%}
<div class="row">
    <div class="col-lg-12">
        
        <div class="clearfix">
            <h1 class="pull-left">Agregar cliente</h1>
           
        </div>
    </div>
</div>
{% endblock -%}
{% block body -%}
<div class="row">
    <div class="col-lg-12">
        <div class="main-box no-header clearfix">
            <div class="main-box-body clearfix">
                {{form_start(form)}}
                <h2>RESUMEN EJECUTIVO</h2>
                <div class="story">
                    {{ form_row(form.empresa) }}
                    {{ form_row(form.puesto) }}
                    {{ form_row(form.nombreCompleto) }}
                </div>
                <h2>AREA EVALUADA</h2>
                <div class="story">
                    {{ form_rest(form) }}
                </div>
            </div>
        </div>
    </div>	
</div>


{% endblock %}
