     */
    public function setMarcoFamiliar4($MarcoFamiliar_4)
    {
        $this->MarcoFamiliar_4 = $MarcoFamiliar_4;

        return $this;
    }

    /**
     * Get MarcoFamiliar_4
     *
     * @return string
     */
    public function getMarcoFamiliar4()
    {
        return $this->MarcoFamiliar_4;
    }


    /**
