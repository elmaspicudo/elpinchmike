{% extends '::base.html.twig' %}

{% block body -%}
    <h1>documentos list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Noseguridadsocial</th>
                <th>Curp</th>
                <th>Compdomiciliario</th>
                <th>Credencialine</th>
                <th>Rfc</th>
                <th>Anonbrede</th>
                <th>Cartaslaborales</th>
                <th>Observaciones</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('documentos_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.noSeguridadSocial }}</td>
                <td>{{ entity.curp }}</td>
                <td>{{ entity.compDomiciliario }}</td>
                <td>{{ entity.credencialINE }}</td>
                <td>{{ entity.rfc }}</td>
                <td>{{ entity.aNonbreDe }}</td>
                <td>{{ entity.cartasLaborales }}</td>
                <td>{{ entity.observaciones }}</td>
                <td>
                <a  href="{{ path('documentos_show', { 'id': entity.id }) }}" class="table-link">
	<span class="fa-stack">
	<i class="fa fa-square fa-stack-2x"></i>
	<i class="fa fa-search-plus fa-stack-1x fa-inverse"></i>
	</span>
</a>
                    <a href="{{ path('documentos_edit', { 'id': entity.id }) }}" class="table-link">
	<span class="fa-stack">
	<i class="fa fa-square fa-stack-2x"></i>
	<i class="fa fa-pencil fa-stack-1x fa-inverse"></i>
	</span>
</a>
                    </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('documentos_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
