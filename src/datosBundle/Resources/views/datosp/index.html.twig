{% extends'::base.html.twig' %}
{% block placeholder -%}
<div class="row">
    <div class="col-lg-12">
        <ol class="breadcrumb">
            <li><a href="#">Home</a></li>
            <li class="active"><span>Solicitudes</span></li> 
        </ol>

    </div>
</div>
{% endblock -%}
{% block body -%}
<div class="row">
<div class="col-lg-12">
<div class="main-box no-header clearfix">
<div class="main-box-body clearfix">
<div class="table-responsive">
<table class="table user-list table-hover">
<thead>
            <tr>
                <th>Id</th>
                <th>Empresa</th>
                <th>Nombre</th>
                <th>Puesto</th>
                <th>Fechadenacimiento</th>
                <th>Fecha</th>
                <th>Nacionalidad</th>
                <th>Domicilio</th>
                <th>Ciudad</th>
                <th>Telefonocasa</th>
                <th>Celular</th>
                <th>Teltrabajo</th>
                <th>Edad</th>
                <th>Sexo</th>
                <th>Lugardenacimiento</th>
                <th>Ecivil</th>
                <th>Colonia</th>
                <th>Pais</th>
                <th>Concluciones</th>
                <th>Resultadodelaentrevista</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('datosp_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.empresa }}</td>
                <td>{{ entity.nombre }}</td>
                <td>{{ entity.puesto }}</td>
                <td>{% if entity.fechaDeNacimiento %}{{ entity.fechaDeNacimiento|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{% if entity.fecha %}{{ entity.fecha|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.nacionalidad }}</td>
                <td>{{ entity.domicilio }}</td>
                <td>{{ entity.ciudad }}</td>
                <td>{{ entity.telefonoCasa }}</td>
                <td>{{ entity.celular }}</td>
                <td>{{ entity.telTrabajo }}</td>
                <td>{{ entity.edad }}</td>
                <td>{{ entity.sexo }}</td>
                <td>{{ entity.lugarDeNacimiento }}</td>
                <td>{{ entity.eCivil }}</td>
                <td>{{ entity.colonia }}</td>
                <td>{{ entity.pais }}</td>
                <td>{{ entity.concluciones }}</td>
                <td>{{ entity.resultadoDeLaEntrevista }}</td>
                <td>
                <a  href="{{ path('datosp_show', { 'id': entity.id }) }}" class="table-link">
	<span class="fa-stack">
	<i class="fa fa-square fa-stack-2x"></i>
	<i class="fa fa-search-plus fa-stack-1x fa-inverse"></i>
	</span>
</a>
                    <a href="{{ path('datosp_edit', { 'id': entity.id }) }}" class="table-link">
	<span class="fa-stack">
	<i class="fa fa-square fa-stack-2x"></i>
	<i class="fa fa-pencil fa-stack-1x fa-inverse"></i>
	</span>
</a>
                    </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('datosp_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
