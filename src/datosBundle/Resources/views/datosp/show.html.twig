{% extends '::base.html.twig' %}

{% block body -%}
    <h1>datosp</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Empresa</th>
                <td>{{ entity.empresa }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ entity.nombre }}</td>
            </tr>
            <tr>
                <th>Puesto</th>
                <td>{{ entity.puesto }}</td>
            </tr>
            <tr>
                <th>Fechadenacimiento</th>
                <td>{{ entity.fechaDeNacimiento|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
                <td>{{ entity.fecha|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Nacionalidad</th>
                <td>{{ entity.nacionalidad }}</td>
            </tr>
            <tr>
                <th>Domicilio</th>
                <td>{{ entity.domicilio }}</td>
            </tr>
            <tr>
                <th>Ciudad</th>
                <td>{{ entity.ciudad }}</td>
            </tr>
            <tr>
                <th>Telefonocasa</th>
                <td>{{ entity.telefonoCasa }}</td>
            </tr>
            <tr>
                <th>Celular</th>
                <td>{{ entity.celular }}</td>
            </tr>
            <tr>
                <th>Teltrabajo</th>
                <td>{{ entity.telTrabajo }}</td>
            </tr>
            <tr>
                <th>Edad</th>
                <td>{{ entity.edad }}</td>
            </tr>
            <tr>
                <th>Sexo</th>
                <td>{{ entity.sexo }}</td>
            </tr>
            <tr>
                <th>Lugardenacimiento</th>
                <td>{{ entity.lugarDeNacimiento }}</td>
            </tr>
            <tr>
                <th>Ecivil</th>
                <td>{{ entity.eCivil }}</td>
            </tr>
            <tr>
                <th>Colonia</th>
                <td>{{ entity.colonia }}</td>
            </tr>
            <tr>
                <th>Pais</th>
                <td>{{ entity.pais }}</td>
            </tr>
            <tr>
                <th>Concluciones</th>
                <td>{{ entity.concluciones }}</td>
            </tr>
            <tr>
                <th>Resultadodelaentrevista</th>
                <td>{{ entity.resultadoDeLaEntrevista }}</td>
            </tr>
        </tbody>
    </table>

        <a  href="{{ path('datosp') }}" class="btn btn-warning">Regresar</a>
        <a href="{{ path('datosp_edit', { 'id': entity.id }) }}" class="btn btn-success">Editar</a>
    {{ form(delete_form) }}
{% endblock %}
