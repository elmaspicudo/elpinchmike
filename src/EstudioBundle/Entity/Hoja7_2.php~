<?php

namespace EstudioBundle\Entity;

/**
 * Hoja7_2
 */
class Hoja7_2
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $nombre;

    /**
     * @var string
     */
    private $tiempoConocer;

    /**
     * @var string
     */
    private $domicilio;

    /**
     * @var integer
     */
    private $telefono;

    /**
     * @var string
     */
    private $comentarios;


    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set nombre
     *
     * @param string $nombre
     *
     * @return Hoja7_2
     */
    public function setNombre($nombre)
    {
        $this->nombre = $nombre;

        return $this;
    }

    /**
     * Get nombre
     *
     * @return string
     */
    public function getNombre()
    {
        return $this->nombre;
    }

    /**
     * Set tiempoConocer
     *
     * @param string $tiempoConocer
     *
     * @return Hoja7_2
     */
    public function setTiempoConocer($tiempoConocer)
    {
        $this->tiempoConocer = $tiempoConocer;

        return $this;
    }

    /**
     * Get tiempoConocer
     *
     * @return string
     */
    public function getTiempoConocer()
    {
        return $this->tiempoConocer;
    }

    /**
     * Set domicilio
     *
     * @param string $domicilio
     *
     * @return Hoja7_2
     */
    public function setDomicilio($domicilio)
    {
        $this->domicilio = $domicilio;

        return $this;
    }

    /**
     * Get domicilio
     *
     * @return string
     */
    public function getDomicilio()
    {
        return $this->domicilio;
    }

    /**
     * Set telefono
     *
     * @param integer $telefono
     *
     * @return Hoja7_2
     */
    public function setTelefono($telefono)
    {
        $this->telefono = $telefono;

        return $this;
    }

    /**
     * Get telefono
     *
     * @return integer
     */
    public function getTelefono()
    {
        return $this->telefono;
    }

    /**
     * Set comentarios
     *
     * @param string $comentarios
     *
     * @return Hoja7_2
     */
    public function setComentarios($comentarios)
    {
        $this->comentarios = $comentarios;

        return $this;
    }

    /**
     * Get comentarios
     *
     * @return string
     */
    public function getComentarios()
    {
        return $this->comentarios;
    }
}
