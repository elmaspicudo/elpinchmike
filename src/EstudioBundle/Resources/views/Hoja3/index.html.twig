                <th>Parentesco</th>
                <th>Observaciones</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('dependienteseconomicos_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
