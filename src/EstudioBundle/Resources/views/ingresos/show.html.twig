{% extends '::base.html.twig' %}

{% block body -%}
    <h1>ingresos</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Investigado</th>
                <td>{{ entity.investigado }}</td>
            </tr>
            <tr>
                <th>Conyuge</th>
                <td>{{ entity.conyuge }}</td>
            </tr>
            <tr>
                <th>Padres</th>
                <td>{{ entity.padres }}</td>
            </tr>
            <tr>
                <th>Hermanos</th>
                <td>{{ entity.hermanos }}</td>
            </tr>
            <tr>
                <th>Otros</th>
                <td>{{ entity.otros }}</td>
            </tr>
        </tbody>
    </table>

        <a  href="{{ path('ingresos') }}" class="btn btn-warning">Regresar</a>
        <a href="{{ path('ingresos_edit', { 'id': entity.id }) }}" class="btn btn-success">Editar</a>
    {{ form(delete_form) }}
{% endblock %}
