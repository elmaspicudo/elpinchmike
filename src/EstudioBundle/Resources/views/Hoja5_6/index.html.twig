{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Hoja5_6 list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Aseguradora</th>
                <th>Tiposeguro</th>
                <th>Mensualidad</th>
                <th>Vigencia</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('hoja5_6_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.aseguradora }}</td>
                <td>{{ entity.tipoSeguro }}</td>
                <td>{{ entity.mensualidad }}</td>
                <td>{% if entity.vigencia %}{{ entity.vigencia|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('hoja5_6_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('hoja5_6_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('hoja5_6_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
